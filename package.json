{
   "name": "snb-api",
   "version": "0.0.1",
   "description": "",
   "author": "",
   "private": true,
   "license": "UNLICENSED",
   "scripts": {
      "prebuild": "rimraf build",
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "NODE_ENV=production node build/main",
      "start:dev": "nest start --watch",
      "start:debug": "NODE_ENV=development nest start --debug --watch",
      "start:prod": "NODE_ENV=production node build/main",
      "start:qa": "NODE_ENV=qa node build/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "pretest": "yarn build && yarn typeorm schema:drop && yarn typeorm migration:run && yarn seed:run",
      "test": "jest",
      "posttest": "yarn typeorm schema:drop",
      "drop": "yarn typeorm schema:drop",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
      "db:migration": "yarn build && yarn typeorm migration:run",
      "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config -n src/ormconfig.ts",
      "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js -n src/ormconfig.ts seed",
      "seed:generate": "ts-node typeorm migration:generate -c seed -n"
   },
   "dependencies": {
      "@googlemaps/google-maps-services-js": "^3.3.4",
      "@nestjs/axios": "^0.0.8",
      "@nestjs/common": "^7.6.15",
      "@nestjs/config": "^0.6.3",
      "@nestjs/core": "^7.6.15",
      "@nestjs/event-emitter": "^1.1.0",
      "@nestjs/jwt": "^7.2.0",
      "@nestjs/mapped-types": "*",
      "@nestjs/passport": "^7.1.5",
      "@nestjs/platform-express": "^7.6.15",
      "@nestjs/typeorm": "^7.1.5",
      "@types/faker": "^5.5.6",
      "@types/joi": "^17.2.3",
      "@types/passport-jwt": "^3.0.5",
      "axios": "^0.27.2",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.13.2",
      "cloudinary": "^1.27.1",
      "cors": "^2.8.5",
      "cpf-cnpj-validator": "^1.0.3",
      "date-fns": "^2.25.0",
      "dotenv": "^10.0.0",
      "eslint-config-airbnb": "^18.2.1",
      "eslint-plugin-import": "^2.23.4",
      "express": "^4.18.2",
      "firebase-admin": "^9.9.0",
      "joi": "^17.5.0",
      "jsonwebtoken": "^8.5.1",
      "jwks-rsa": "^2.1.3",
      "jwt-decode": "^3.1.2",
      "multer": "^1.4.5-lts.1",
      "mysql": "^2.14.1",
      "passport": "^0.4.1",
      "passport-jwt": "^4.0.0",
      "pg": "^8.6.0",
      "reflect-metadata": "^0.1.10",
      "rimraf": "^3.0.2",
      "rxjs": "^6.6.6",
      "slugify": "^1.6.5",
      "source-map-support": "^0.5.21",
      "stripe": "^8.215.0",
      "typeorm": "0.2.32",
      "typeorm-seeding": "^1.6.1",
      "typescript": "^4.2.4"
   },
   "devDependencies": {
      "@nestjs/cli": "^7.6.0",
      "@nestjs/schematics": "^7.3.0",
      "@nestjs/testing": "^7.6.15",
      "@types/express": "^4.17.11",
      "@types/express-session": "^1.17.0",
      "@types/jest": "^26.0.22",
      "@types/multer": "^1.4.7",
      "@types/node": "^14.14.33",
      "@types/supertest": "^2.0.10",
      "@typescript-eslint/eslint-plugin": "^4.19.0",
      "@typescript-eslint/parser": "^4.19.0",
      "eslint": "^7.22.0",
      "eslint-config-prettier": "^8.1.0",
      "eslint-plugin-prettier": "^3.3.1",
      "jest": "^26.6.3",
      "prettier": "^2.2.1",
      "supertest": "^6.1.3",
      "ts-jest": "^26.5.4",
      "ts-loader": "^8.0.18",
      "ts-node": "3.3.0",
      "tsconfig-paths": "^3.9.0"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}
